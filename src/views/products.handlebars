<li><a href="/">Inicio</a></li>
<h2>Todos los productos</h2>

<a href="/form-products">Crear productos</a>

<table class="tabla">
  <thead>
    <tr class="columna">
      <th>ID</th>
      <th>Título</th>
      <th>Descripción</th>
      <th>Precio</th>
      <th>Imagen</th>
      <th>Stock</th>
      <th>Código</th>
    </tr>
  </thead>

  {{#each products}}
    <tr>
      <td>{{this.id}}</td>
      <td>{{this.title}}</td>
      <td>{{this.description}}</td>
      <td>{{this.price}}</td>
      <td>{{this.thumbnail}}</td>
      <td>{{this.stock}}</td>
      <td>{{this.code}}</td>
    </tr>
  {{/each}}
</table>